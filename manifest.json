{
  "description": "Limits tabs to keep things tidy",
  "manifest_version": 3,
  "name": "Limit Tabs",
  "version": "3.0.0",
  "background": {
    "service_worker": "limittabsto2.js"
  },
  "options_ui": {
    "page": "options.html"
  },
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": {
      "32": "icons/enabled.png"
    },
    "default_title": "Toggle Limit Tabs"
  },
  "icons": {
    "32": "icons/enabled.png",
    "128": "icons/enabled.png"
  }
}
